{
  "!name": "requirejs.js",
  "!plugin": "requireJS",
  "!predef": {
    "requirejs": {
      "!type": "fn(deps: [string], callback: fn(), errback: fn()) -> $custom:requireJS",
      "onError": "fn(err: +Error.prototype)",
      "load": "fn(context: ?, moduleName: string, url: string)",
      "config": "fn(config: ?)",
      "version": "string",
      "isBrowser": "bool"
    }
  },
  "requirejs": "requirejs",
  "require": "requirejs",
  "define": {
    "!type": "fn(deps: [string], callback: fn()) -> $custom:requireJS",
    "amd": {
      "jQuery": "bool"
    }
  }
}
